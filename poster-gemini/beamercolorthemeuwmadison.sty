% Gemini theme
% https://github.com/anishathalye/gemini

% ====================
% Definitions
% ====================

% Colors for web from: https://brand.wisc.edu/web/colors/
% Colors for print from: https://brand.wisc.edu/print/colors/
\definecolor{BadgerRedPrint}{cmyk}{0.03,1,0.66,0.12}
\definecolor{BadgerRedDarkerPrint}{cmyk}{0.24,1,0.78,0.17}
\definecolor{BadgerRedDarkestPrint}{cmyk}{0.09, 1, 0.64, 0.4}

% Extra colors
\definecolor{lightgray}{RGB}{240, 240, 240}
\definecolor{lightorange}{RGB}{255, 245, 242}

% ====================
% Theme
% ====================

% Basic colors
\setbeamercolor{palette primary}{fg=black,bg=white}
\setbeamercolor{palette secondary}{fg=black,bg=white}
\setbeamercolor{palette tertiary}{bg=black,fg=white}
\setbeamercolor{palette quaternary}{fg=black,bg=white}
\setbeamercolor{structure}{fg=BadgerRedDarkerPrint}

% Headline
\setbeamercolor{headline}{fg=white,bg=BadgerRedDarkerPrint}

% Block
\setbeamercolor{block title}{fg=BadgerRedDarkerPrint,bg=white}
\setbeamercolor{block separator}{bg=black}
\setbeamercolor{block body}{fg=black,bg=white}

% Alert Block
\setbeamercolor{block alerted title}{fg=BadgerRedDarkerPrint,bg=lightorange}
\setbeamercolor{block alerted separator}{bg=black}
\setbeamercolor{block alerted body}{fg=black,bg=lightorange}

% Example Block
\setbeamercolor{block example title}{fg=BadgerRedDarkerPrint,bg=lightgray}
\setbeamercolor{block example separator}{bg=black}
\setbeamercolor{block example body}{fg=black,bg=lightgray}

% Heading
\setbeamercolor{heading}{fg=black}

% Itemize
\setbeamercolor{item}{fg=BadgerRedDarkerPrint}

% Bibliography
\setbeamercolor{bibliography item}{fg=black}
\setbeamercolor{bibliography entry author}{fg=black}
\setbeamercolor{bibliography entry title}{fg=black}
\setbeamercolor{bibliography entry location}{fg=black}
\setbeamercolor{bibliography entry note}{fg=black}
